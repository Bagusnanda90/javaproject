		ctrlist.addHeader("Competencies","");
		ctrlist.addHeader("Score","");
		ctrlist.addHeader("Level","");
		ctrlist.addHeader("Note/Special Achv.","");

		ctrlist.setLinkRow(0);
		ctrlist.setLinkSufix("");
		Vector lstData = ctrlist.getData();
		Vector lstLinkData = ctrlist.getLinkData();
		ctrlist.setLinkPrefix("javascript:cmdEditCompetencies('");
		ctrlist.setLinkSufix("')");
		ctrlist.reset();
		int index = -1;
                int no=0; 
		for (int i = 0; i < objectClass.size(); i++) {
                        no++;
			EmployeeCompetency empCompetency = (EmployeeCompetency)objectClass.get(i);
			 Vector rowx = new Vector();
			 if(competencyID == empCompetency.getOID())
				 index = i;
                        Competency competency = empCompetency.getCompetency()!=null?  empCompetency.getCompetency() : new Competency();
			
                        CompetencyLevel competencyLevel=  empCompetency.getCompLevel()!=null?  empCompetency.getCompLevel() : new CompetencyLevel();
			if(empCompetency.getCompetencyLevelId()!=0){
                            try{
                                competencyLevel = PstCompetencyLevel.fetchExc(empCompetency.getCompetencyLevelId());
                            }catch(Exception exc){
                               competencyLevel=  new CompetencyLevel();  
                            }
                        }
			rowx.add(""+no);
			rowx.add(""+competency.getCompetencyName() + "<br>"+ competency.getDescription());
			rowx.add(""+empCompetency.getScoreValue());
			rowx.add(""+competencyLevel.getDescription()+" " + " | " +competencyLevel.getLevelMin()+" ~ "+competencyLevel.getLevelMax() +" " + competencyLevel.getLevelUnit()); 
			rowx.add(""+empCompetency.getSpecialAchievement());
			lstData.add(rowx);
			lstLinkData.add(String.valueOf(empCompetency.getOID())); 
		}

		return ctrlist.draw(index);
	}

